program CotarFrete;

uses
  Vcl.Forms,
  View.Principal in 'View\View.Principal.pas' {Form1},
  View.ConsultaCnpj in 'View\View.ConsultaCnpj.pas' {ConsultaCnpj},
  View.ConsultasMelhorEnvio in 'View\View.ConsultasMelhorEnvio.pas' {ConsultasMelhorEnvio},
  View.ConsultaCredencialAzul in 'View\View.ConsultaCredencialAzul.pas' {Form2};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TConsultaCnpj, ConsultaCnpj);
  Application.CreateForm(TConsultasMelhorEnvio, ConsultasMelhorEnvio);
  Application.CreateForm(TForm2, Form2);
  Application.Run;
end.
